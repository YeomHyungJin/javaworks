package forexample;

public class Gugudan {

	public static void main(String[] args) {
		// 전체 구구단 - 9행 1열(중첩 for)
		for (int j = 2 ; j < 10 ; j++)
		{
			System.out.println(j + "단");
			for(int h = 1; h < 10 ; h++)
			{
				System.out.println(j + "x" + h + "=" + (j*h));
			}
			System.out.println();
		}
	}
}
