package classpart;

public class StudentTest {

	public static void main(String[] args) {
		// 학생 클래스를 사용
		//클래스 인스턴스변수 = new 생성자 
		Student s1 = new Student();
		s1.studentName = "염형진";
		s1.grade = 3;
		s1.showInfo();
		
		Student s2 = new Student();
		s2.studentName = "인희영";
		s2.grade = 2;
		s2.showInfo();
		
		Student s3 = new Student("홍길동");
		
		System.out.println(s1);
		System.out.println(s2);
	}
}
