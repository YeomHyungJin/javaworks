package operator;

public class MultiOP {

	public static void main(String[] args) {
		// 복합 대입 연산자
		int num1 = 10;
		int num2 = 20;
		int num3 = 30;
		
		System.out.println(num1 += 2);  // num = num + 2
		System.out.println(num2 /= 10); // num = num % 10
		System.out.println(num3 -= 1);  // num = num - 1
	}
}
