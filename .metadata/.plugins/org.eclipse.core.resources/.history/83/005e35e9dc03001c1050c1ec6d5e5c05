package operator;

public class MultiOP {

	public static void main(String[] args) {
		
		// 조건 연산자
		// 조건식 ? 참인 값 : 거짓인 값
		int val1 = (5 > 3) ? 10 : 20;
		boolean val2 = (5 < 3) ? true : false;
		System.out.println(val1);
		System.out.println(val2);
		
		
		
		int fatherAge = 45;
		int motherAge = 47;
		char ch;
		
		ch = (fatherAge > motherAge) ? 'T' : 'F';
		
		System.out.println(ch);
	}
}
